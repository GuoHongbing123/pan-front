this.$route.params.id\r\n        this.check()\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.s-container{\r\n    .check-container{\r\n        display: flex;\r\n        align-items: center;\r\n        height: 100vh;\r\n        justify-content: center;\r\n        .check-box{\r\n            width: 350px;\r\n            border-radius: 16px;\r\n            border: 1px solid #f6f6f6;\r\n            padding: 35px 35px 15px;\r\n            box-shadow: 0 0 25px #aaa;\r\n        }\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/views/Share"}]}