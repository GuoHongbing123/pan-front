,QAAA,YAAA,CAAA;AACA,UAAA,GAAA,EAAA,QAAA,GAAA,QAAA,GAAA,CADA;AAEA,UAAA,QAAA,EAAA;AAFA,SAAA,CAAA,CAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,YAAA,KAAA,CAAA,IAAA,CAAA,KAAA,CAAA,SAAA;AACA,WAHA,MAGA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,SAVA;AAWA,OAlBA,EAkBA,KAlBA,CAkBA,YAAA,CAEA,CApBA;AAqBA,KAlCA;AAmCA,IAAA,QAnCA,sBAmCA;AAAA;;AACA,MAAA,MAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OAPA;AAQA,KA5CA;AA6CA,IAAA,QA7CA,sBA6CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA,KA/CA;AAgDA,IAAA,MAhDA,oBAgDA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA;AAlDA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <el-aside class=\"aside\" width=\"250px\">\r\n      <div class=\"nav-warrper\">\r\n        <div class=\"nav-item\" @command=\"handleCommand\" @click=\"handleNav(item)\" v-for=\"item in navList\" :key=\"item.title\">{{item.title}}</div>\r\n      </div>\r\n  </el-aside>\r\n</template>\r\n\r\n<script>\r\nimport { logout } from \"../../api/user\";\r\nimport { createFolder } from \"../../api/folder\";\r\nexport default {\r\n data(){\r\n     return{\r\n         navList:[\r\n             {\r\n                 title:'我的文件',\r\n                 clickHandler:'home'\r\n             },\r\n             {\r\n                 title:'新建文件夹',\r\n                 clickHandler:'newFolder'\r\n             },\r\n            {\r\n                 title:'回收站',\r\n                 clickHandler:'recycle'\r\n             },\r\n              {\r\n                 title:'登出',\r\n                 clickHandler:'loginout'\r\n             },\r\n            {\r\n                title:'修改密码',\r\n                clickHandler:'password'\r\n            },\r\n            {\r\n                title:'个人信息',\r\n                clickHandler:'detail'\r\n            },\r\n         ]\r\n\r\n     }\r\n },\r\n methods:{\r\n     handleCommand(clickHandler){\r\n         this[clickHandler]()\r\n     },\r\n     handleNav(item) {\r\n         let routes =['home','recycle']\r\n         if (routes.includes(item.clickHandler)){\r\n             this.$router.push(item.clickHandler)\r\n         }else{\r\n             this[item.clickHandler]()\r\n         }\r\n     },\r\n     newFolder(){\r\n         this.$prompt('请输入新建文件夹名称', '新建', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          inputPattern: /^((?![\\\\/:*?？^`~&<>|'%]).){1,20}$/,\r\n          inputErrorMessage: '20个字符以内，不能包含特殊字符'\r\n        }).then(({ value }) => {\r\n          let folderId=this.$route.query.id\r\n          createFolder({\r\n              pid:folderId?folderId:0,\r\n              realName:value\r\n          }).then(res=>{\r\n              if(res.code===0){\r\n                  this.$message.success('新建成功')\r\n                  this.$bus.$emit('getlist')\r\n              }else{\r\n                  this.$message.error(res.msg)\r\n              }\r\n          })\r\n        }).catch(() => {\r\n                \r\n        });\r\n     },\r\n     loginout(){\r\n         logout().then(res=>{\r\n             if(res.code===0){\r\n                 this.$message.success('登出成功')\r\n                 this.$router.push('/login')\r\n             }else{\r\n                 this.$message.error(res.msg)\r\n             }\r\n         })\r\n     },\r\n    password(){\r\n        this.$router.push('../forget')\r\n    },\r\n    detail(){\r\n        this.$router.push('../detail')\r\n    }\r\n }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.aside{\r\n    // height:calc (100vh - 60px) ;\r\n    background-color: #fff;\r\n    margin-bottom:10px;\r\n    box-shadow:inset 5px 5px 5px rgb(0 0 0/10%);\r\n    .nav-warrper{\r\n        .nav-item{\r\n            height: 50px;\r\n            cursor: pointer;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: center;\r\n            font-size: 14px;\r\n            background: #5ce0f1;\r\n            &:hover{\r\n                background-color: #9df0ff;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/views/Main"}]}