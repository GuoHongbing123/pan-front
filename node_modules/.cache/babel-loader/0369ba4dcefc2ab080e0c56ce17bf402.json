AA,EAAA,IAAA,KAAA,GAAA,IAAA,MAAA,CAAA,UAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,UAAA,CAAA,IAAA,CAAA,EAAA;AACA;AACA;AACA;AACA,SAbA;AAcA;AACA,KAjBA;AAkBA,GA9DA;AA+DA,EAAA,QA/DA,oBA+DA,OA/DA,EA+DA,KA/DA,EA+DA;AACA,QAAA,MAAA,GAAA,OAAA,IAAA,EAAA;AACA,QAAA,IAAA,GAAA,KAAA,IAAA,EAAA;AACA,IAAA,MAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,QAAA,GAAA,QAAA;AACA,KAFA;AAGA,IAAA,IAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,OAAA,CAAA,MAAA,CAAA;AACA,MAAA,OAAA,CAAA,QAAA,GAAA,IAAA,GAAA,IAAA,GAAA,SAAA;AACA,MAAA,OAAA,CAAA,IAAA,GAAA,IAAA,CAAA,UAAA,CAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA;AACA,KAJA;AAKA,SAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA;AACA,GA3EA;AA4EA,EAAA,QA5EA,oBA4EA,IA5EA,EA4EA;AAAA;;AACA,IAAA,eAAA,CAAA,IAAA,CAAA,EAAA,EAAA,IAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,8BAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,OAFA,MAEA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,KANA;AAOA,GApFA;AAqFA,EAAA,IArFA,gBAqFA,IArFA,EAqFA;AAAA;;AACA,SAAA,UAAA,GAAA,IAAA;AACA,SAAA,QAAA,GAAA,IAAA;AACA,SAAA,SAAA,CAAA,YAAA;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,aAAA,GAAA,IAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA;AAIA,GA5FA;AA6FA,EAAA,UA7FA,sBA6FA,QA7FA,EA6FA;AAAA;;AACA,QAAA,IAAA,GAAA;AACA,MAAA,cAAA,EAAA;AADA,KAAA;AAGA,IAAA,IAAA,CAAA,KAAA,QAAA,CAAA,IAAA,KAAA,GAAA,GAAA,UAAA,GAAA,QAAA,CAAA,GAAA,KAAA,QAAA,CAAA,EAAA;AACA,IAAA,KAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,OAJA,MAIA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,KARA;AASA;AA3GA,CA7BA,wEA0IA;AACA,OAAA,IAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,OAAA,SAAA;AACA,CA7IA","sourcesContent":["<template>\r\n  <div class=\"s-container\">\r\n      <div class=\"check-container\" v-show=\"!hasCheck\">\r\n          <el-form :model=\"form\" class=\"check-box\" ref=\"form\" :rules=\"rules\">\r\n              <el-form-item>\r\n                  <p style=\"font-size:20px; font-weigth:blod\">{{`${shareInfo.ownerName}`}}向您分享了文件</p>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                  <el-input placeholder=\"请输入提取码\" maxlength=\"4\" v-model=\"form.code\"></el-input>\r\n              </el-form-item>\r\n              <el-form-item>\r\n                  <el-button type=\"info\" @click=\"checkCode\" style=\"width: 100%\">提取文件</el-button>\r\n              </el-form-item>\r\n          </el-form>\r\n      </div>\r\n      <el-main class=\"main-box\">\r\n          <div class=\"title\">\r\n              {{title}}\r\n          </div>\r\n          <div class=\"share-time\">\r\n              <i class=\"el-icon-time\"></i>\r\n              <span>{{shareTime}}</span>\r\n          </div>\r\n          <div class=\"breadcrumb-box\">\r\n              <div class=\"breadcrumb-item\" v-for=\"(item, index) in breadcrumb\" :key=\"item.id\">\r\n                  <div class=\"item-box\" @click=\"handleBreadcrumb(item, index)\">\r\n                      <i class=\"el-icon-s-home\" style=\"font-size: 20px\" v-if=\"index === 0\"></i>\r\n                      <span style=\"font-size: 16px\" v-else>{{item.realName}}</span>\r\n                  </div>\r\n                  <i class=\"el-icon-arrow-right\" style=\"font-size: 18px\" v-if=\"index !==breadcrumb.length-1\"></i>\r\n              </div>\r\n          </div>\r\n          <div class=\"file-list\">\r\n              <el-table :data=\"list\">\r\n                  <el-table-column label=\"名称\"  min-width=\"300\">\r\n                      <template slot-scope=\"scope\">\r\n                          <div class=\"name\" @click=\"checkChildren(scope.row)\">\r\n                            <img class=\"icon\" :src=\"require(`@/assets/img/${scope.row.iconType}-icon.svg`)\"/>\r\n                            <span :style=\"{cursor:scope.row.type === '2' ? 'pointer' : 'auto'}\" style=\"font-size: 16px\">{{scope.row.realName}}</span>\r\n                          </div>\r\n                      </template>\r\n                  </el-table-column>\r\n                  <el-table-column label=\"大小\" prop=\"size\">\r\n                  </el-table-column>\r\n                  <el-table-column label=\"操作\">\r\n                      <template slot-scope=\"scope\">\r\n                          <el-button @click=\"save(scope.row)\" type=\"text\">保存</el-button>\r\n                          <el-button @click=\"download(scope.row)\" type=\"text\">下载</el-button>\r\n                      </template>\r\n                  </el-table-column>\r\n              </el-table>\r\n          </div>\r\n      </el-main>\r\n      <select-folder v-if=\"selectShow\" @saveConfirm=\"handleSave\" :isMove=\"false\" ref=\"select\"></select-folder>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { checkShare,authShare,Tsave,getChildren } from \"../../api/share\";\r\nimport fileType from \"../../const/fileType\"\r\nimport Tool from \"../../utils/tool\"\r\nimport { getDownloadPath } from \"../../api/file\";\r\nimport SelectFolder from \"../../components/SelectFolder.vue\";\r\n\r\nexport default {\r\n  components: { SelectFolder },\r\n    data(){\r\n        return {\r\n            breadcrumb:[],\r\n            link:'',\r\n            hasCheck:false,\r\n            needAuth:true,\r\n            title:'',\r\n            shareTime:'',\r\n            shareInfo:{\r\n                ownerName: ''\r\n            },\r\n            form:{\r\n                code:''\r\n            },\r\n            list:[],\r\n            rules:{\r\n                code:[{\r\n                    required:true,message:'请输入提取码',trigger:'blur'\r\n                }],\r\n            },\r\n            selectShow:false,\r\n            saveInfo:null,\r\n        }\r\n    },\r\n    components:{\r\n        SelectFolder\r\n    },\r\n    methods:{\r\n        handleBreadcrumb(item,index){\r\n            if(this.breadcrumb.length>1){\r\n                if(index === 0){\r\n                    this.checkCode()\r\n                }else{\r\n                    this.checkChildren(item, true)\r\n                }\r\n                this.breadcrumb.splice(index + 1, this.breadcrumb.length)\r\n            }\r\n        },\r\n        checkChildren(item, noPush){\r\n            if(item.type==='2'){\r\n                getChildren(this.link,this.form.code,item.id).then(res=>{\r\n                    if(res.code===0){\r\n                        this.dealFile(res.data.folder,res.data.file)\r\n                        if(!noPush){\r\n                            this.breadcrumb.push(item)\r\n                        }\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        initCheck(){\r\n            checkShare(this.link).then(res=>{\r\n                if(res.code===0){\r\n                    this.needAuth=res.data.shareType==='1'\r\n                    this.shareInfo=res.data\r\n                    if(this.needAuth){\r\n                        let code = localStorage.getItem(`#${this.link}`)\r\n                        if(code){\r\n                            this.form.code = code\r\n                            this.checkCode()\r\n                        }\r\n                    }else{\r\n                        this.hasCheck=true\r\n                    }\r\n                }\r\n            })\r\n        },\r\n        saveLink(){\r\n            localStorage.setItem(`#${this.link}`,this.form.code)\r\n        },\r\n        checkCode(){\r\n            this.$refs.form.validate((valid)=>{\r\n                if(valid){\r\n                    authShare(this.link,this.form.code).then(res=>{\r\n                        if(res.code===0){\r\n                            this.hasCheck=true\r\n                            if(this.needAuth){\r\n                                this.saveLink()\r\n                                this.dealFile(res.data.folder,res.data.file)\r\n                                this.title=this.list[0].realName\r\n                                this.shareTime=this.list[0].updateTime\r\n                                if(this.list[0].type === '2' && this.breadcrumb.length === 0){\r\n                                    this.breadcrumb.push({})\r\n                                }\r\n                            }\r\n                        }\r\n                    })\r\n                }\r\n            })\r\n        },\r\n        dealFile(folder_, file_){\r\n            let folder=folder_ || []\r\n            let file=file_ || []\r\n            folder.forEach(element=>{\r\n                element.iconType= 'folder'\r\n            });\r\n            file.forEach(element=>{\r\n                let type = fileType[element.suffix]\r\n                element.iconType= type ? type : 'unknown'\r\n                element.size= Tool.formatSize(Number(element.size))\r\n            })\r\n            this.list=folder.concat(file)\r\n        },\r\n        download(item){\r\n            getDownloadPath(item.id, item.type).then(res=>{\r\n                if(res.code===0){\r\n                    window.open('http://101.34.26.36:8080/'+res.data.dlLink)\r\n                }else{\r\n                    this.$message.error(res.msg)\r\n                }\r\n            })\r\n        },\r\n        save(item){\r\n            this.selectShow=true\r\n            this.saveInfo=item\r\n            this.$nextTick(()=>{\r\n                this.$refs.select.dialogVisible=true\r\n                this.$refs.select.init(item)\r\n            })\r\n        },\r\n        handleSave(folderId){\r\n            let data={\r\n                targetFolderId:folderId\r\n            }\r\n            data[this.saveInfo.type==='2' ? 'folderId' : 'fileId'] = this.saveInfo.id\r\n            Tsave(data).then(res=>{\r\n                if(res.code===0){\r\n                    this.selectShow=false\r\n                    this.$message.success('保存成功')\r\n                    this.saveInfo=null\r\n                }else{\r\n                    this.$message.error(res.msg)\r\n                }\r\n            })\r\n        }\r\n    },\r\n    mounted(){\r\n        this.link=this.$route.params.id\r\n        this.initCheck()\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.s-container{\r\n    .check-container{\r\n        display: flex;\r\n        align-items: center;\r\n        height: 100vh;\r\n        justify-content: center;\r\n        .check-box{\r\n            width: 350px;\r\n            border-radius: 16px;\r\n            border: 1px solid #f6f6f6;\r\n            padding: 35px 35px 15px;\r\n            box-shadow: 0 0 25px #aaa;\r\n        }\r\n    }\r\n    .main-box{\r\n        padding-top: 40px;\r\n        .title{\r\n            font-size: 20px;\r\n            font-weight: bold;\r\n        }\r\n        .share-time{\r\n            margin: 20px 0;\r\n            span{\r\n                font-size: 14px;\r\n            }\r\n            i{\r\n                margin-right: 10px;\r\n                font-size: 14px;\r\n            }\r\n        }\r\n        .breadcrumb-box{\r\n            display: flex;\r\n            .breadcrumb-item{\r\n                display: flex;\r\n                align-items: center;\r\n                .item-box{\r\n                    cursor: pointer;\r\n                    padding: 5px;\r\n                    display: flex;\r\n                    align-items: center;\r\n                    &:hover{\r\n                        background-color: #f5f5ff;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        .file-list{\r\n            .name{\r\n                display: flex;\r\n                align-items: center;\r\n                .icon{\r\n                    width: 24px;\r\n                    height: 24px;\r\n                    margin-right: 10px;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n</style>"],"sourceRoot":"src/views/Share"}]}